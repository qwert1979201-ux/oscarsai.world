{
    "name": "oscarsai-monorepo",
    "title": "oscarsai",
    "version": "25.1.0",
    "description": "oscarsai Monorepo",
    "author": {
        "name": "oscarsai Teams",
        "url": "https://www.oscarsai.world
"
    },
    "license": "Apache-2.0",
    "homepage": "https://www.oscarsai.world
",
    "repository": {
        "type": "git",
        "url": "https://github.com/BidingCC/oscarsai.git"
    },
    "keywords": [
        "oscarsai",
        "oscarsai-monorepo",
        "oscarsai-refactor",
        "ai framework",
        "artificial intelligence",
        "machine learning",
        "deep learning",
        "nlp",
        "natural language processing",
        "computer vision",
        "neural networks",
        "javascript ai",
        "typescript ai",
        "vue ai",
        "ai development",
        "ai tools",
        "ai agent",
        "mcp agent",
        "mcp tool",
        "mcp framework"
    ],
    "scripts": {
        "build": "cross-env NODE_OPTIONS=--max-old-space-size=5120 turbo run build",
        "dev": "turbo run dev",
        "start": "pnpm i && pnpm sync-env && cd packages/cli && pnpm run start",
        "predeploy": "pnpm i && pnpm sync-env && cd packages/cli && pnpm run predeploy",
        "stop": "node packages/cli/bin/cli.js pm2:stop",
        "restart": "node packages/cli/bin/cli.js pm2:restart",
        "delete": "node packages/cli/bin/cli.js pm2:delete",
        "lint": "turbo run lint",
        "lint:fix": "turbo run lint:fix",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "typecheck": "turbo run check-types",
        "clean": "rimraf ./.turbo && turbo run clean",
        "sync-env": "node scripts/sync-env.mjs",
        "oscarsai": "node packages/cli/bin/cli.js",
        "pm2:start": "node packages/cli/bin/cli.js pm2:start",
        "pm2:reset": "rimraf logs/pm2 && pnpm delete && pnpm start",
        "pm2:reload": "node packages/cli/bin/cli.js pm2:reload",
        "pm2:status": "node packages/cli/bin/cli.js pm2:status",
        "pm2:logs": "node packages/cli/bin/cli.js pm2:logs",
        "pm2:monitor": "node packages/cli/bin/cli.js pm2:monitor",
        "pm2:save": "node packages/cli/bin/cli.js pm2:save",
        "pm2:flush": "node packages/cli/bin/cli.js pm2:flush",
        "bd:update": "node packages/cli/bin/cli.js update",
        "bd:update-git": "node packages/cli/bin/cli.js update-git",
        "docker:up": "docker compose up -d",
        "docker:down": "docker compose down",
        "dev:desktop": "cd packages/desktop && pnpm run dev",
        "dev:api": "cd packages/api && pnpm run dev",
        "dev:web": "cd packages/web/oscarsai-ui && pnpm run dev",
        "dev:main": "cd packages/web/oscarsai-ui && pnpm run dev && cd ../../api && pnpm run dev",
        "build:desktop": "cd packages/desktop && pnpm run build:desktop",
        "build:api": "cd packages/api && pnpm run build",
        "build:web": "cd packages/web/oscarsai-ui && pnpm run generate"
    },
